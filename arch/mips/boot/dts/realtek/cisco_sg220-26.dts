/dts-v1/;

#include "rtl83xx.dtsi"
#include "rtl838x.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/mux/mux.h>

/ {
	model = "Cisco SG220-26";
	compatible = "cisco,sg220-26", "realtek,rtl8382-soc";

	chosen {
		stdout-path = "serial0";
		bootargs = "earlycon console=ttyS0,115200";
	};

	aliases {
		mdio-gpio1 = &mdio1;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <10>;
		debounce-interval = <50>;

		reset {
			gpios = <&gpio1 27 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-restart {
		compatible = "gpio-restart";
		gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		priority = <192>;
		open-source;
	};

	mdio1: mdio {
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_mdx_pins>;

		compatible = "virtual,mdio-gpio";
		#address-cells = <1>;
		#size-cells = <0>;
		gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>,
			<&gpio0 3 GPIO_ACTIVE_HIGH>;

		gpio1: expander@31 {
			compatible = "realtek,rtl8231", "syscon";
			reg = <31>;

			gpio-controller;
			#gpio-cells = <2>;
			gpio-ranges = <&gpio1 0 0 37>;
		};
	};

	virtual_mux: mux-virtual {
		compatible = "mux-virtual";
		num-states = 5;
		idle-state = <MUX_IDLE_AS_IS>;
	};

	i2c-mux {
		compatible = "i2c-mux";
		mux-controls = <&virtual_mux>;

		#address-cells = <1>;
		#size-cells = <0>;

		/* Temperature sensor bus */
		i2c-gpio@0 {
			compatible = "i2c-gpio";
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;
			scl-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>;
			sda-gpios = <&gpio1 29 GPIO_ACTIVE_HIGH>;
			i2c-gpio,scl-open-drain;
			i2c-gpio,sda-open-drain;

			sensor@48 {
				compatible = "national,lm75a";
				reg = <0x48>;
			};

			sensor@4b {
				compatible = "national,lm75a";
				reg = <0x4b>;
			};
		};

		/* Fan controller 1 bus */
		i2c-gpio@1 {
			compatible = "i2c-gpio";
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
			scl-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>;
			sda-gpios = <&gpio1 33 GPIO_ACTIVE_HIGH>;
			i2c-gpio,scl-open-drain;
			i2c-gpio,sda-open-drain;


		};

		/* Fan controller 2 bus */
		i2c-gpio@2 {
			compatible = "i2c-gpio";
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;
			scl-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>;
			sda-gpios = <&gpio1 31 GPIO_ACTIVE_HIGH>;
			i2c-gpio,scl-open-drain;
			i2c-gpio,sda-open-drain;
		};

		/* SFP25 bus */
		i2c-gpio@3 {
			compatible = "i2c-gpio";
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;
			scl-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>;
			sda-gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
			i2c-gpio,scl-open-drain;
			i2c-gpio,sda-open-drain;
		};

		/* SFP26 bus */
		i2c-gpio@4 {
			compatible = "i2c-gpio";
			reg = <4>;
			#address-cells = <1>;
			#size-cells = <0>;
			scl-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>;
			sda-gpios = <&gpio1 14 GPIO_ACTIVE_HIGH>;
			i2c-gpio,scl-open-drain;
		};
	};

	switchcore@1b000000 {
		compatible = "syscon", "simple-mfd";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <0x0 0x1b000000 0x20000>;

		pinctrl_sysled: pinmux@a000 {
			compatible = "pinctrl-single";
			reg = <0xa000 0x4>;

			pinctrl-single,bit-per-mux;
			pinctrl-single,register-width = <32>;
			pinctrl-single,function-mask = <0x1>;
			#pinctrl-cells = <2>;

			sys_led_pins: pinmux_sys_led_pins {
				pinctrl-single,bits = <0x0 0x0000 0x8000>;
			};
		};

		pinctrl_gpio: pinmux@a0e0 {
			compatible = "pinctrl-single";
			reg = <0xa0e0 0x4>;

			pinctrl-single,bit-per-mux;
			pinctrl-single,register-width = <32>;
			pinctrl-single,function-mask = <0x1>;
			#pinctrl-cells = <2>;

			gpio_mdx_pins: pinmux_gpio_mdx_pins {
				pinctrl-single,bits = <0x0 0x0 0x1>;
			};
		};
	};
};

&uart0 {
	status = "okay";
};
